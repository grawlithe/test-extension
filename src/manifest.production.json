{
    "manifest_version": 2,
    "name": "MyExtGPT",
    "description": "My Chatgpt extension",
    "version": "0.0.1",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "/content.js"
            ]
        }
    ]
}